{
  "parser ": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "generators": true
  },
  "rules": {
    "babel/transform-async-to-generator": 1,
    "no-console": 0
  },
  "plugins": [
    "babel"
  ],
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended"
}